{
  "compilerOptions": {
    "incremental": true,
    "lib": [
      "ESNext", "DOM"
    ],
    "target": "esnext",
    "module": "commonjs",
    "declaration": true,
    "sourceMap": true,
    "composite": true,
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "rootDir": "src",
    "outDir": "public/static",
    "jsx": "react",
    "noImplicitAny": false
  }
}