{
  "name": "node-weather-agent",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm install",
    "build": "tsc --build",
    "prestart": "npm run build",
    "start": "node --env-file .env ./dist/index.js",
    "test-tool": "teamsapptester start",
    "test": "npm-run-all -p -r start test-tool",
    "prebundle": "npm run build",
    "bundle": "esbuild ./src/index.js --bundle --platform=node --metafile=dist/meta.json --outfile=dist/bundle.cjs --tree-shaking=true --keep-names",
    "predocker": "npm run bundle",
    "docker": "docker build -t \"$npm_package_name:$npm_package_version\" ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "description": "",
  "dependencies": {
    "@langchain/langgraph": "^0.2.57",
    "@langchain/openai": "^0.5.10",
    "@microsoft/agents-hosting": "^0.3.0",
    "express": "^5.0.1"
  },
  "devDependencies": {
    "@microsoft/teams-app-test-tool": "^0.2.8",
    "@types/node": "^22.13.4",
    "esbuild": "^0.25.0",
    "nodemon": "^3.1.9",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.7.2"
  }
}
